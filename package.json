{
  "name": "edealer-estimator-web",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "scripts": {
    "clean": "rimraf dist coverage",
    "prebuild": "rimraf dist coverage",
    "build": "webpack --config etc/webpack.js",
    "analyze": "ANALYZE_BUNDLE=true webpack --config etc/webpack.js",
    "test": "jest $FLAGS",
    "start": "stmux -M -- [ -t \"test watch\" \"jest --watch --notify --coverage\" : -s 1/3 -t \"webpack log\" \"webpack-dev-server --hot --config etc/webpack.js\" ]",
    "test:ui": "yarn build; testcafe $(if [ \"$BROWSER\" = \"\" ]; then BROWSER=all; fi; echo $BROWSER) $FLAGS test"
  },
  "jest": {
    "globals": {
      "__APP__": {
        "root": "edealer-estimator-test"
      }
    },
    "transform": {
      "\\.jsx?$": "babel-jest",
      "\\.svg$": "<rootDir>/src/lib/test-utils/benignTransform.js"
    }
  },
  "dependencies": {
    "autobind-decorator": "^2.1.0",
    "core-js": "^2.5.5",
    "prop-types": "^15.6.1",
    "raf": "^3.4.0",
    "react": "^16.3.0",
    "react-dom": "^16.3.0",
    "react-hot-loader": "^4.0.1",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2",
    "styled-components": "^3.2.5"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.11",
    "jest": "^22.4.3",
    "jest-junit": "^3.7.0",
    "jest-styled-components": "^5.0.1",
    "react-test-renderer": "^16.3.1",
    "rimraf": "^2.6.2",
    "stmux": "^1.4.19",
    "svg-react-loader": "^0.4.5",
    "testcafe": "^0.19.2",
    "testcafe-reporter-xunit": "^2.1.0",
    "webpack": "^4.4.1",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.3"
  }
}
